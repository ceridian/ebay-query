app.set('port', process.env.PORT || 3000);

models.sequelize.sync().then(function () {
  var server = http.createServer(app);
  io = require("socket.io").listen(server);
  server.listen(3000);
  require('../lib/io.js');
  console.log(3000);
  io.messageSummary("jake's test");
});
